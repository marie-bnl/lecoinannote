{
  "manifest_version": 2,

  "name": "lecoinannote",
  "version": "1.0",
  "description": "Annoter les offres leboncoin.",

  "permissions": ["storage"],

  "content_scripts": [
    {
      "matches": ["*://*.leboncoin.fr/carte/*"],
      "js": ["dist/map.js"]
    },
    {
      "matches": ["*://*.leboncoin.fr/ad/*"],
      "js": ["dist/ad.js"]
    }
  ]
}
